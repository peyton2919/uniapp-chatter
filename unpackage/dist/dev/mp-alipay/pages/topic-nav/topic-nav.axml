<view><scroll-view class="scroll-row" style="height:100rpx;" scroll-x="{{true}}" scroll-into-view="{{scrollIntoView}}" scroll-with-animation="{{true}}"><view class="{{((((('scroll-row-item')+' '+'px-3')+' '+'py-2')+' '+'font-md')+' '+(tabIndex===index?'text-main font-lg font-weight-bold':''))}}" id="{{'tab'+index}}" data-event-opts="{{[['tap',[['changeTab',[index]]]]]}}" onTap="__e" a:for="{{tabBars}}" a:for-item="item" a:for-index="index" a:key="index">{{''+item.name+''}}</view></scroll-view><swiper class="swiper" style="{{('height:'+scrollH+'px;')}}" duration="{{150}}" current="{{tabIndex}}" data-event-opts="{{[['change',[['onChangeTab',['$event']]]]]}}" onChange="__e"><swiper-item a:for="{{newsList}}" a:for-item="item" a:for-index="index" a:key="index"><scroll-view style="{{('height:'+scrollH+'px;')}}" scroll-y="{{true}}" data-event-opts="{{[['scrollToLower',[['loadmore',[index]]]]]}}" onScrollToLower="__e"><block a:if="{{item.list.length>0}}"><block a:for="{{item.list}}" a:for-item="item2" a:for-index="index2" a:key="index2"><topic-list vue-id="{{'2fd7ee0a-1-'+index+'-'+index2}}" item="{{item2}}" index="{{index2}}" onVueInit="__l"></topic-list><divider vue-id="{{'2fd7ee0a-2-'+index+'-'+index2}}" onVueInit="__l"></divider></block><load-more vue-id="{{'2fd7ee0a-3-'+index}}" str="{{item.loadmore}}" onVueInit="__l"></load-more></block><block a:else><no-thing vue-id="{{'2fd7ee0a-4-'+index}}" str="什么都没有" onVueInit="__l"></no-thing></block></scroll-view></swiper-item></swiper></view>