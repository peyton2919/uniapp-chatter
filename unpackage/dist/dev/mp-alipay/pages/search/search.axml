<view><block a:if="{{searchList.length===0}}"><view class="py-2 font-md px-2">搜索历史</view><view class="flex flex-wrap"><view class="border rounded font mx-2 my-1 px-2 py-2 " hover-class="bg-light" id="{{index}}" data-event-opts="{{[['tap',[['clickSearchHistory',['$0'],[[['list','',index]]]]]]]}}" onTap="__e" a:for="{{list}}" a:for-item="item" a:for-index="index">{{item}}</view></view></block><block a:else><block a:for="{{searchList}}" a:for-item="item" a:for-index="index" a:key="index"><common-list vue-id="{{'50c0d8d6-1-'+index}}" item="{{item}}" index="{{index}}" onVueInit="__l"></common-list></block></block></view>