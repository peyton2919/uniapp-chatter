<view><view class="flex align-center h-100"><view data-event-opts="{{[['tap',[['changeTab',[index]]]]]}}" class="{{((((('flex')+' '+'align-end')+' '+'justify-center')+' '+'flex-1')+' '+(index===tabIndex?'font-lg font-weight-bold text-main':'font-md text-dark'))}}" onTap="__e" a:for="{{tabBars}}" a:for-item="item" a:for-index="index" a:key="index">{{''+item.name+''}}<block a:if="{{item.num>0}}"><text class="ml-2 font">{{item.num}}</text></block></view></view><swiper class="swiper" style="{{('height:'+scrollH+'px;')}}" duration="{{150}}" current="{{tabIndex}}" data-event-opts="{{[['change',[['onChangeTab',['$event']]]]]}}" onChange="__e"><swiper-item a:for="{{newsList}}" a:for-item="item" a:for-index="index" a:key="index"><scroll-view style="{{('height:'+scrollH+'px;')}}" scroll-y="{{true}}" data-event-opts="{{[['scrollToLower',[['loadmore',[index]]]]]}}" onScrollToLower="__e"><block a:if="{{item.list.length>0}}"><block a:for="{{item.list}}" a:for-item="item2" a:for-index="index2" a:key="index2"><user-list vue-id="{{'11e62c58-1-'+index+'-'+index2}}" item="{{item2}}" index="{{index2}}" onVueInit="__l"></user-list></block><block a:if="{{item.list.length>10}}"><load-more vue-id="{{'11e62c58-2-'+index}}" str="{{item.loadmore}}" onVueInit="__l"></load-more></block></block><block a:else><no-thing vue-id="{{'11e62c58-3-'+index}}" str="什么都没有" onVueInit="__l"></no-thing></block></scroll-view></swiper-item></swiper></view>